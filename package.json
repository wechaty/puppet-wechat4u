{
  "name": "wechaty-puppet-wechat4u",
  "version": "0.19.4",
  "description": "Wechat4u Puppet for Wechaty",
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/esm/src/mod.js",
      "require": "./dist/cjs/src/mod.js"
    }
  },
  "types": "./dist/esm/src/mod.d.ts",
  "engines": {
    "wechaty": ">=0.68",
    "node": ">=16"
  },
  "scripts": {
    "build": "tsc && tsc -p tsconfig.cjs.json",
    "clean": "shx rm -fr dist/*",
    "dist": "npm-run-all clean build dist:commonjs",
    "dist:commonjs": "jq -n \"{ type: \\\"commonjs\\\" }\" > dist/cjs/package.json",
    "lint": "npm-run-all lint:es lint:ts lint:md",
    "lint:md": "markdownlint README.md",
    "lint:ts": "tsc --isolatedModules --noEmit",
    "lint:es": "eslint \"src/**/*.ts\" \"tests/**/*.spec.ts\" --ignore-pattern tests/fixtures/",
    "test": "npm run lint && npm run test:unit",
    "test:pack": "bash -x scripts/npm-pack-testing.sh",
    "test:unit": "tap --node-arg=--loader=ts-node/esm --node-arg=--no-warnings \"src/**/*.spec.ts\" \"tests/**/*.spec.ts\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/wechaty/wechaty-puppet-wechat4u.git"
  },
  "keywords": [
    "chatie",
    "wechaty",
    "wechat",
    "chatbot",
    "bot",
    "sdk",
    "puppet",
    "wechat4u"
  ],
  "author": "Huan LI <zixia@zixia.net>",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/wechaty/wechaty-puppet-wechat4u/issues"
  },
  "devDependencies": {
    "@chatie/eslint-config": "^0.14.1",
    "@chatie/git-scripts": "^0.6.2",
    "@chatie/semver": "^0.4.7",
    "@chatie/tsconfig": "^0.20.2",
    "@types/lru-cache": "^5.1.1",
    "@types/promise-retry": "^1.1.3",
    "memory-card": "^0.12.2",
    "npm-run-all": "^4.1.5",
    "pkg-jq": "^0.2.11",
    "shx": "^0.3.3",
    "tstest": "^0.5.16",
    "wechaty": "^0.69.47"
  },
  "peerDependencies": {
    "wechaty-puppet": ">=0.40"
  },
  "homepage": "https://github.com/wechaty/wechaty-puppet-wechat4u#readme",
  "dependencies": {
    "lru-cache": "^6.0.0",
    "promise-retry": "^2.0.1",
    "wechat4u": "^0.7.7"
  },
  "publishConfig": {
    "access": "public",
    "tag": "next"
  },
  "files": [
    "bin/",
    "dist/",
    "src/"
  ],
  "tap": {
    "check-coverage": false
  },
  "git": {
    "scripts": {
      "pre-push": "npx git-scripts-pre-push"
    }
  }
}
